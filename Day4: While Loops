import java.util.*;

public class Loops{
    public static void main(String args[]){
        int counter=1;

        while(counter<100){
            System.out.println("Hello World");
            counter++;
        }
    }
}

import java.util.*;

public class Loops{
    public static void main(String args[]){
        int num=1;

        while(num<=10){
            System.out.println(num);
            num=num+1;//num++
        }


    }
}



import java.util.*;

public class Loops{
    public static void main(String args[]){
        Scanner sc= new Scanner(System.in);
        int n= sc.nextInt();
        int num=1;

        while(num<=n){
            System.out.println(num);
            num++;
        }
    }
}




import java.util.*;

public class Loops{
    public static void main(String args[]){
        int sum=0;
        int num=1;
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        while(num<=n){
            sum=sum+num;
            num++;    
        }
         System.out.println(sum);

    }
}
