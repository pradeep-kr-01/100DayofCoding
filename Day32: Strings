import java.util.*;

public class string{
    public static void main(String[] args) {
        char arr[]={'a','b','c'};
        String str = "abcd";
        String str2 = new String("abcd");

        //String are IMMUTABLE

        Scanner sc = new Scanner(System.in);
        String name;
        name = sc.nextLine();
        System.out.println(name);

        //length function
        System.out.println(str.length());
    }
}
