public class Loops{
    public static void main (String[]args) {
        int counter = 1;
        do {
            System.out.println("Hello World");
            counter++;

        } while (counter <=10);
    }
}


import java.util.*;

public class Loops{
    public static void main(String args[]){

        for(int i=1; i<=5; i++){
            if (i==3) {
                break;
            }
            System.out.println(i);
        }

        System.out.println("I am out of the loop");
    }
}


import java.util.*;

public class Loops{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);

        do { 
            System.out.print("Enter your number");
            int n = sc.nextInt();

            if(n%10==10){
                break;
            }
            System.out.println(n);
        } while (true);
        

    }
}


import java.util.*;

public class Loops{
    public static void main(String args[]){
        for (int i=1; i<=5; i++){
            if(i==3){
                continue;
            }
            System.out.println(i);
        }

    }
}


import java.util.*;

public class Loops{
    public static void main(String args[]){

        Scanner sc = new Scanner(System.in);

        do { 
            System.out.print("Enter your number: ");
            int n = sc.nextInt();

            if (n%10==0){
                continue;
            }
            System.out.println(n);
        } while(true);
    }
}
